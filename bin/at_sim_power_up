#!/usr/bin/env python

import sys
import HeaderService.SAL_tools as SAL_tools
if __name__ == "__main__":
    commands = sys.argv[1:]
    if len(commands) < 1:
        commands = ['enterControl','start','enable']
        print("Will execute Power-up Sequence:")
    SAL_tools.command_sequencer(commands,Device='atHeaderService')
