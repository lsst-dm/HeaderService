#!/usr/bin/env python3

import sys
import salpytools
if __name__ == "__main__":
    commands = sys.argv[1:]
    if len(commands) < 1:
        commands = ['Disable', 'Standby', 'ExitControl']
        print("Will execute Power-down Sequence:")
    salpytools.command_sequencer(commands, Device='dmHeaderService')
