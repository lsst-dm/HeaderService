#!/usr/bin/env python

import sys
import HeaderService.SAL_tools_send as SAL_tools_send

if __name__ == "__main__":

    commands = sys.argv[1:]
    if len(commands) < 1:
        commands = ['Disable','Standby','ExitControl']
        print("Will execute Power-down Sequence:")
    SAL_tools_send.command_sequencer(commands)
