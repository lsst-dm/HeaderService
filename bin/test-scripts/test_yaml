#!/usr/bin/env python

import yaml
telem = yaml.load(open('telemetry.yaml'))

print telem

channels = []
for key in telem:
    c = {'device':telem[key]['device'],    
         'topic': telem[key]['topic'],
         'Stype' :telem[key]['Stype']}
    if c not in channels:
        channels.append(c)

for c in channels:
    print c
exit()

