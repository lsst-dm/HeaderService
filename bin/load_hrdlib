#!/usr/bin/env python

import MHClient

if __name__ == "__main__":

    # Load a set of random words and values
    # we can access it via w.words[k] and w.words_values[k]
    w = MHClient.RANWORDS()

    # Load up the HDR templates
    t = MHClient.HDRTEMPL()

    print t.header['RA']
    print t.header['DEC']
    print t.header['VISITID']

    # Update next visit  -- we could loop over this
    t.next_visit()

    print t.header['RA']
    print t.header['DEC']
    print t.header['VISITID']

    exit()
    
    # Loop over all of the contents
    for name in t.header.keys():

        # To access the record itself
        #print t.get_record(name)
        print name, t.header.get(name), t.header.get_comment(name)


        
