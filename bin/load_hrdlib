    h.next_visit()
    s2 = h.get_stream()


    print s2


    exit()


    print h.stream_keys
    print h.stream_vals
    s = h.get_stream()
    print h.stream_vals

    print s
    
    exit()

    print h.words
    print h.words_vals

    print h.header.keys()
    print h.header
    exit()

    print h.header['RA']
    print h.header['DEC']
    print h.header['VISITID']

    h.next_visit()

    print h.header['RA']
    print h.header['DEC']
    print h.header['VISITID']
    
    # Loop over all of the contents
    for name in h.header.keys():

        # To access the record itself
        #print t.get_record(name)
        print name, h.header.get(name), h.header.get_comment(name)


        
